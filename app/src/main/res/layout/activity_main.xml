<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:toolbar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".app.ui.viewImp.MainActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:background="@color/color_01A1ED"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        toolbar:navigationIcon="@mipmap/ic_toolbar_drawer"
        toolbar:title="@string/toolbar_title"
        toolbar:titleTextColor="@color/color_FFFFFF"
        >
    </android.support.v7.widget.Toolbar>
</LinearLayout>
